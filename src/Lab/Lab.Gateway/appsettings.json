{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "modern-api": {
        "ClusterId": "modern-api-cluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      },
      "legacy-quote": {
        "ClusterId": "legacy-quote-cluster",
        "Match": {
          "Path": "/legacy/quote/{**catch-all}"
        }
      },
      "legacy-policy": {
        "ClusterId": "legacy-policy-cluster",
        "Match": {
          "Path": "/legacy/policy/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "modern-api-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:6001"
          }
        }
      },
      "legacy-quote-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:6002"
          }
        }
      },
      "legacy-policy-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:6003"
          }
        }
      }
    }
  }
}

